<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="pl.mimuw.zpp.guestBook.domain">

	<class name="Entry" table="Entry">
	    <id name="id" type="long" column="ENTRY_ID">
	        <generator class="native"/>
	    </id>
	    
	    <property name= "name"/>
	    
	    <property name= "email"/>
	    
	    <property name= "text"/>
	    
	    <property name= "date"/>
	</class>
    

</hibernate-mapping>