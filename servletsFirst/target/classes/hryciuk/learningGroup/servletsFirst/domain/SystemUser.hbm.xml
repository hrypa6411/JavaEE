<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="hryciuk.learningGroup.servletsFirst.domain">

	<class name="SystemUser" table="SystemUser">
	    <id name="idUser" type="long" column = "SystemUserId">
	        <generator class="native"/>
	    </id>
	    
	    <property name= "loginUser"/>
	    
	    <property name= "passwordUser"/>
	</class>
    

</hibernate-mapping>
